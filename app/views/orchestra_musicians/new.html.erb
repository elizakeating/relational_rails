<%= form_with url: "/orchestras/#{@orchestra.id}/musicians", method: :post, data: { turbo: false } do |form| %>
  <%= form.label :name, "Musician Name:" %><br/>
  <%= form.text_field :name %><br/>
  <br><%= form.label :full_time, "Full Time:" %><br/>
  <%= form.select :full_time, ["true", "false"] %><br/>
  <br><%= form.label :years_involved, "Years Involved:" %><br/>
  <%= form.number_field :years_involved %><br/>
  <br><%= form.submit "Create Musician" %>
<% end %>