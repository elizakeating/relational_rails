<%= link_to "Welcome Page", "/" %>
<%= link_to "Musicians Index", "/musicians" %>
<%= link_to "Orchestras Index", "/orchestras" %>

<h1>Orchestras Index</h1>

<%= link_to "New Orchestra", "/orchestras/new" %>

<% @orchestras.each do |orchestra| %>
  <h3><%= link_to "#{orchestra.name}", "/orchestras/#{orchestra.id}" %></h3>
  <p>Created At: <%= orchestra.created_at %><p>
  <%= link_to "Update #{orchestra.name}", "/orchestras/#{orchestra.id}/edit" %><br/>
  <%= button_to "Delete #{orchestra.name}", "/orchestras/#{orchestra.id}", method: :delete %>
  <p>~*~*~*~*~*~*~*~*~*~*~*~*~*~*~*~</p>
<% end %>
