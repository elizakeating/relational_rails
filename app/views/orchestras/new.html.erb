<%= form_with url: "/orchestras", method: :post, data: { turbo: false } do |form| %>
  <%= form.label :name, "Orchestra Name:" %><br/>
  <%= form.text_field :name %><br/>
  <br/><%= form.label :auditions_open, "Auditions Open:" %><br/>
  <%= form.select :auditions_open, ["true", "false"] %><br/>
  <br/><%= form.label :year_established, "Year Established:" %><br/>
  <%= form.number_field :year_established %><br/>
  <br/><%= form.submit 'Create Orchestra' %>
<% end %>
